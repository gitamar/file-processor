{
  "name": "file-processor",
  "version": "0.0.1",
  "description": "",
  "author": "gdgamamarnath@gmail.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "(npm run copy-dts) & (npm run copy-js) & (nest build)",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "(npm run copy-dts) & (npm run copy-js) & (nest start --watch) ",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
    "copy-js": "copyfiles -u 1 \"src/**/*.js\" dist"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^8.0.6",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.2",
    "amqp-connection-manager": "^3.6.0",
    "amqplib": "^0.8.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "copyfiles": "^2.4.1",
    "google-protobuf": "^3.18.0",
    "json2csv": "^5.0.6",
    "jsontoxml": "^1.0.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "ts-protoc-gen": "^0.15.0",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/google-protobuf": "^3.15.5",
    "@types/jest": "^27.0.1",
    "@types/json2csv": "^5.0.3",
    "@types/jsontoxml": "^1.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/xml2json": "^0.11.4",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.83.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
